{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
      "strict": true,
      "baseUrl": "./src",
      "paths": {
        "@assets/*": ["src/assets/*"],
        "@components/*": ["src/components/*"],
        "@routes/*": ["src/routes/*"],
        "@screens/*": ["src/screens/*"],
        "@storage/*": ["src/storage/*"],
        "@utils/*": ["src/utils/*"]
      },
    }
  }
